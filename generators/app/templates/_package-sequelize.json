{
  "name": "<%= name %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "author": "<%= author %>",
  "main": "src/index.js",
  "private": false,
  "license": "<%= license %>",
  "engines": {
    "node": ">=8",
    "yarn": "*"
  },
  "scripts": {
    "precommit": "yarn validate",
    "prestart": "yarn docs",
    "start": "cross-env NODE_ENV=production pm2 start ./src/index.js",
    "dev": "NODE_ENV=development nodemon ./src/index.js",
    "lint": "eslint '**/*.js' --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "yarn lint -- --fix",
    "lint:watch": "yarn lint -- --watch",
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "test": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles --coverage --coverageDirectory=coverage",
    "test:unit": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles --testPathPattern=\"(/src/.*)\\.(spec).js$\"",
    "pretest:integration": "NODE_ENV=test sequelize db:migrate",
    "test:integration": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles --testPathPattern=\"(/src/.*)\\.(test).js$\"",
    "test:watch": "cross-env NODE_ENV=test jest --watch --forceExit --detectOpenHandles",
    "validate": "yarn lint && yarn test",
    "postpublish": "git push --tags",
    "docs": "apidoc -i src -o docs",
    "docker:start": "cross-env NODE_ENV=production pm2-docker start ./src/index.js",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up --abort-on-container-exit",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "passport",
    "es6",
    "es7",
    "es8",
    "es2017",
    "jest",
    "eslint",
    "REST",
    "API",
    "boilerplate",
    "generator",
    "starter project",
    "aws"
  ],
  "dependencies": {
    "async": "^2.6.1",
    "aws-sdk": "^2.253.1",
    "axios": "^0.18.0",
    "axios-retry": "^3.1.1",
    "bcryptjs": "2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.0",
    "celebrate": "^8.0.0",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "cross-env": "^5.0.1",
    "dotenv-safe": "^5.0.1",
    "express": "^4.15.2",
    "express-interceptor": "^1.2.0",
    "express-validation": "^1.0.2",
    "helmet": "^3.5.0",
    "http-status": "^1.0.1",
    "joi": "^10.4.1",
    "jws": "^3.1.5",
    "jwt-simple": "0.5.1",
    "lodash": "^4.17.4",
    "method-override": "^2.3.8",
    "module-alias": "^2.0.6",
    "moment-timezone": "^0.5.13",
    "mongoose": "^4.9.7",
    "node-sanitizer": "^1.0.3",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "4.0.0",
    "pg": "^7.6.0",
    "pg-hstore": "^2.3.2",
    "pm2": "^2.4.6",
    "response-time": "^2.3.2",
    "sequelize": "^4.41.0",
    "uuid": "^3.1.0",
    "winston": "^3.1.0",
    "winston-cloudwatch": "^2.0.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.3",
    "apidoc": "^0.17.6",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "eslint": "^5.6.1",
    "eslint-config-astro": "^1.0.6",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "jest-express": "^1.8.0",
    "jest-tobetype": "^1.2.0",
    "nodemon": "^1.11.0",
    "opn-cli": "^3.1.0",
    "prettier-eslint": "^8.8.2",
    "proxyquire": "^2.1.0",
    "sequelize-cli": "^5.2.0",
    "supertest": "^3.0.0"
  },
  "_moduleAliases": {
    "@app"          : "src",
    "@config"       : "src/config",
    "@api"          : "src/api",
    "@middlewares"  : "src/middlewares",
    "@models"       : "src/models",
    "@services"     : "src/services",
    "@utils"        : "src/utils"
  },
  "nyc": {
    "include": "src",
    "exclude": [
      "**/*.spec.js",
      "**/*.test.js",
      "**/index.js"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^@app(.*)$": "<rootDir>/src$1",
      "^@boot(.*)$": "<rootDir>/src/boot$1",
      "^@config(.*)$": "<rootDir>/src/config$1",
      "^@api(.*)$": "<rootDir>/src/api$1",
      "^@middlewares(.*)$": "<rootDir>/src/middlewares$1",
      "^@models(.*)$": "<rootDir>/src/models$1",
      "^@services(.*)$": "<rootDir>/src/services$1",
      "^@utils(.*)$": "<rootDir>/src/utils$1"
    },
    "testRegex": "(/src/.*)\\.(test|spec).js$",
    "moduleFileExtensions": [
      "js"
    ],
    "testPathIgnorePatterns": [
      "\\index.js$",
      "(/src/.*)\\.(validation|config|index).js$"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/config/**",
      "!src/**/index.js",
      "!src/database/**",
      "!src/models/**",
      "!src/**/*.validation.js",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "json",
      "html",
      "lcov",
      "text",
      "clover"
    ]
  }
}
